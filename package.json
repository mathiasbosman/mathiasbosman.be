{
  "homepage": "https://mathiasbosman.github.io/",
  "license": "MIT",
  "description": "Personal resume",
  "version": "3.14.3",
  "author": "Mathias Bosman",
  "repository": {
    "type": "git",
    "url": "git://github.com/mathiasbosman/mathiasbosman.github.io.git"
  },
  "scripts": {
    "prebuild": "node ./scripts/preInstall.js",
    "build": "npm run prebuild && GENERATE_SOURCEMAP=false react-scripts build",
    "clean": "rm -rf node_modules build",
    "start": "node server/server.js",
    "local": "react-scripts start",
    "test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint '**/*.{js,ts}'",
    "prepRelease": "npm version prerelease --preid='rc' -m 'Prepare next development iteration %s'",
    "minorRelease": "npm version minor -m 'Prepare minor release %s'",
    "prepPatch": "npm version patch --preid='rc' -m 'Prepare patch release %s'"
  },
  "dependencies": {
    "@primer/octicons-react": "^17.5.0",
    "@primer/react": "^35.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "express": "^4.18.1",
    "express-rate-limit": "^6.6.0",
    "react": "^17.0.2",
    "react-content-loader": "^6.2.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.4.1",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint-plugin-primer-react": "^0.7.4",
    "eslint-plugin-react": "^7.31.1",
    "prop-types": "^15.7.2",
    "sass": "^1.56.1"
  }
}
