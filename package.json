{
  "homepage": "https://mathiasbosman.be/",
  "name": "lit-sandbox",
  "private": true,
  "version": "4.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "local": "vite --host",
    "build": "tsc && vite build",
    "postbuild": "node ./scripts/postBuild.cjs",
    "preview": "vite preview",
    "lint": "eslint src/**/*.tsx",
    "lint:fix": "eslint src/**/*.tsx --fix",
    "test:unit": "vitest run",
    "test:e2e": "cypress run --project test/driver/cypress",
    "test:component": "cypress run --project test/driver/cypress --component",
    "test:ui": "cypress open --project test/driver/cypress"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.17.1",
    "eslint": "^8.45.0",
    "eslint-config-standard-with-typescript": "^36.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "postcss": "^8.4.25",
    "sass": "^1.63.6",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.3",
    "vitest": "^0.33.0"
  }
}
