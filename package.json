  "name": "mathiasbosman.be",
  "private": true,
  "version": "4.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "local": "vite --host",
    "build": "tsc && vite build",
    "postbuild": "node ./scripts/postBuild.cjs",
    "preview": "vite preview",
    "lint": "eslint '**/*.tsx'",
